generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Movies {
  title    String?
  director String?
  year     Int?      @db.SmallInt
  series   String?
  date     DateTime? @db.Date
  notes    String?
  id       Int       @id @default(autoincrement())
}
