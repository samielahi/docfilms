generator client {
  provider = "prisma-client-js"
  previewFeatures = ["fullTextSearch"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Movies {
  id       BigInt    @id
  title    String?
  director String?
  year     Int?      @db.SmallInt
  series   String?
  date     DateTime? @db.Date
  notes    String?
}
