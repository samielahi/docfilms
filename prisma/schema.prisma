generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model movies {
  id            Int       @id @default(autoincrement())
  title         String?
  director      String?
  year          Int?      @db.SmallInt
  series        String?
  date          DateTime? @db.Date
  tmdbID        Int?
  times_shown   Int?      @db.SmallInt
  quarter       String?
  overview      String?
  backdrop_path String?
}
